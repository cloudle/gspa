template(name="apiDocumentation")
  .dual-layout#api
    .detail-wrapper
      .dual-detail
        h1 Đây là API!
        h2 Nơi khách hàng cập nhật,
    .dual-addon
      .filter.icon-search-8
        input(name='apiFilter' type="text" placeholder="tìm kiếm..")
      ul
        each apiNodes
          +apiNode

template(name="apiNode")
  li {{name}}
  if childNodes.length
    ul
      each childNodes
        +apiNode nodeDetails